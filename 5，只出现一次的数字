class Solution {
    public int singleNumber(int[] nums) {
        Set<Integer> set = new HashSet<>(nums.length);
        for(int a:nums){
            if(!set.add(a))
            {
                set.remove(a);
            }
    //     set.forEach(item->{
	// 	if(!set.add(item)){
	// 		set.remove(item);
	// 	}return set.iterator().next();
	// });
                
        }return set.iterator().next();

    }
}
